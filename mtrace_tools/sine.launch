<launch>
  <!-- Sinewave generator -->
  <node name="$(anon spawner-sine)" pkg="pr2_controller_manager" type="spawner" output="screen" args="sine"/>
  <rosparam>
    sine:
      type: SineController
      joint_name: dk_joint1
      frequency: 4.00
      amplitude: 0.01
      offset:    0.00
  </rosparam>

  <!-- Publishing trigger -->
  <node name="$(anon spawner-trigger)" pkg="pr2_controller_manager" type="spawner" output="screen" args="trigger"/>
  <rosparam>
    trigger:
      type: DigitalTriggerController
      digital_out_name: dk_motor_publish_motor_trace
  </rosparam>

</launch>
