<launch>
  <include file="$(find qualification)/tests/init.machine" />

  <!-- configure_mcbs -->
  <node machine="test_host" pkg="qualification"
        type="mcb_prog_conf.py" name="r_full_arm_conf"
        args="-c -n 8 --motor=r_shoulder_pan_motor,0 --motor=r_shoulder_lift_motor,1 --motor=r_upper_arm_roll_motor,2 --motor=r_elbow_flex_motor,3 --motor=r_forearm_roll_motor,4 --motor=r_wrist_l_motor,5 --motor=r_wrist_r_motor,6 --motor=r_gripper_motor,7" />

  <!-- Confirm configuation -->
  <node pkg="qualification" type="confirm_conf.py"
        name="confirm_conf" output="screen"/>
</launch>
